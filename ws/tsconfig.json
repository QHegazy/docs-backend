{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "strict": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "sourceMap": true,
      "resolveJsonModule": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true
    },
    "include": ["src/**/*", "src/models"],
    "exclude": ["node_modules", "dist"]
  }